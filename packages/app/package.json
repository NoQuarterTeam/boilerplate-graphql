{
  "name": "@boilerplate/app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint": "eslint --cache --ext .ts,.tsx,.js ./",
    "lint:fix": "eslint --fix --config .eslintrc.commit.js --cache --ext .ts,.tsx,.js ./",
    "prettify": "prettier --write ./ --ignore-path ../../.prettierignore",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write --ignore-path ../../.prettierignore"
    ],
    "*.(js|ts|tsx)|!graphql.tsx": [
      "eslint --fix --config .eslintrc.commit.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.7.0-beta.6",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/core": "6.2.2",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/native-stack": "6.7.0",
    "@react-navigation/stack": "6.2.2",
    "expo": "45.0.8",
    "expo-status-bar": "1.4.0",
    "graphql": "15.8.0",
    "graphql-tag": "2.12.6",
    "native-base": "3.4.11",
    "polished": "4.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.4.4",
    "react-native-web": "0.18.7",
    "styled-components": "5.3.5",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@types/react": "17.0.48",
    "@types/react-dom": "17.0.17",
    "@types/react-native": "0.69.5",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "private": true
}
