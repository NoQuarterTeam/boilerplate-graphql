{
  "private": true,
  "name": "@boilerplate/web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write"
    ],
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix --config .eslintrc.commit.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.4.17",
    "@chakra-ui/react": "1.7.0",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@hookform/resolvers": "2.8.3",
    "@sentry/nextjs": "6.14.3",
    "dayjs": "1.10.7",
    "framer-motion": "5.3.0",
    "graphql": "15.7.2",
    "next": "12.0.3",
    "next-transpile-modules": "9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "11.4.2",
    "react-hook-form": "7.19.4",
    "react-icons": "4.3.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.0",
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.1",
    "@types/cookie": "0.4.1",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "eslint-config-next": "12.0.3"
  }
}
