{
  "private": true,
  "name": "@boilerplate/web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src --ignore-path ../../.prettierignore",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix --config .eslintrc.commit.js"
    ],
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write --ignore-path ../../.prettierignore"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.7.0-beta.6",
    "@chakra-ui/react": "1.8.8",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@hookform/resolvers": "2.9.7",
    "@sentry/nextjs": "7.11.0",
    "dayjs": "1.11.5",
    "framer-motion": "7.2.0",
    "graphql": "15.8.0",
    "next": "12.2.5",
    "next-http-proxy-middleware": "1.2.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "14.2.2",
    "react-hook-form": "7.34.2",
    "react-icons": "4.4.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@types/cookie": "0.5.1",
    "@types/react": "17.0.48",
    "@types/react-dom": "17.0.17",
    "eslint-config-next": "12.2.5"
  }
}
