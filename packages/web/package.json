{
  "private": true,
  "name": "@boilerplate/web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src --ignore-path ../../.prettierignore",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix --config .eslintrc.commit.js"
    ],
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write --ignore-path ../../.prettierignore"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@chakra-ui/react": "^2.0.0-next.2",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@hookform/resolvers": "2.8.8",
    "@sentry/nextjs": "6.19.6",
    "dayjs": "1.11.1",
    "framer-motion": "6.3.0",
    "graphql": "15.8.0",
    "next": "12.1.5",
    "next-http-proxy-middleware": "1.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "12.0.5",
    "react-hook-form": "7.30.0",
    "react-icons": "4.3.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@types/cookie": "0.5.1",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "eslint-config-next": "12.1.5"
  }
}
