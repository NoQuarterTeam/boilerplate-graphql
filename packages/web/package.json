{
  "private": true,
  "name": "@boilerplate/web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write"
    ],
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix --config .eslintrc.commit.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@chakra-ui/react": "1.6.9",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@hookform/resolvers": "2.8.1",
    "@sentry/nextjs": "6.13.2",
    "dayjs": "1.10.7",
    "framer-motion": "4.1.17",
    "graphql": "15.6.1",
    "next": "11.1.2",
    "next-transpile-modules": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "11.4.2",
    "react-hook-form": "7.17.1",
    "react-icons": "4.3.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.6",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@types/cookie": "0.4.1",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "eslint-config-next": "11.1.2"
  }
}
